      <div class="container">
        <!-- Article List -->
        <div id="article-list">
          <% @result_array.each do |result| %>
            <div class="article-list-title">
              <h3><%= link_to result["title"], result["link"],:target=>["_blank"] %></h3>
              <div class="<%= result["desc_style"] %>">
                <h5><span class="glyphicon glyphicon-transfer"> <%= result["title_ja"] %></h5>
                <p class="text-muted"><small><%= result["japaneseDesc"] %></small></p>
              </div>
              <div>
                <span class="<%= result["label_style"] %>"><%= result["feedname"] %></span> <strong><%= result["author"] %></strong> <%= result["published_date"] %>
                </div>
            </div>
          <% end %>
        </div>
      </div>

